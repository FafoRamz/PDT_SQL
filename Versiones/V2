/*====================================*/
/*       		EQUIPO TF5    		       	*/
/*   		CODIGO SQL PROYECTO      	*/
/*====================================*/

/*====================================*/
/*    Conectado a la BD desde usuario SYSTEM	*/
/*	Ejecutamos los siguientes comandos	*/
/*====================================*/

--Damos permisos para crear el usuario PROYECTO
ALTER SESSION SET "_ORACLE_SCRIPT"=true;

-- Creamos el usuario PROYECTO, con contrase√±a PROYECTO 
CREATE USER PROYECTO 
IDENTIFIED BY PROYECTO 
DEFAULT TABLESPACE USERS;

--  Damos permisos DBA
GRANT DBA  TO PROYECTO;

/*====================================*/
--BORRADO DE TABLAS 
/*====================================*/
DROP TABLE ITR CASCADE CONSTRAINTS;
DROP TABLE ANALISTAS CASCADE CONSTRAINTS;
DROP TABLE ESTUDIANTES CASCADE CONSTRAINTS;
DROP TABLE TUTORES CASCADE CONSTRAINTS;
DROP TABLE TIPO_TUTOR CASCADE CONSTRAINTS;  
DROP TABLE LOCALIDADES CASCADE CONSTRAINTS;
DROP TABLE EVENTOS CASCADE CONSTRAINTS;
DROP TABLE USUARIOS CASCADE CONSTRAINTS;
DROP TABLE DEPARTAMENTOS CASCADE CONSTRAINTS;
DROP TABLE ACCION_CONSTANCIA CASCADE CONSTRAINTS;
DROP TABLE ACCION_JUSTIFICACION CASCADE CONSTRAINTS;
DROP TABLE ACCION_RECLAMO CASCADE CONSTRAINTS;
DROP TABLE CONSTANCIAS CASCADE CONSTRAINTS;
DROP TABLE CONVOCATORIA_ASISTENCIA CASCADE CONSTRAINTS;
DROP TABLE JUSTIFICACIONES CASCADE CONSTRAINTS;
DROP TABLE RECLAMOS CASCADE CONSTRAINTS;
DROP TABLE TIPO_ASISTENCIA CASCADE CONSTRAINTS;
DROP TABLE GESTIONES CASCADE CONSTRAINTS;
DROP TABLE RESPONSABILIDADES CASCADE CONSTRAINTS;




/*====================================*/
--CREACION DE LAS TABLAS
/*====================================*/

-- Creamos la tabla DEPARTAMENTOS
CREATE TABLE DEPARTAMENTOS (
ID_DEPARTAMENTO INTEGER NOT NULL,
NOMBRE_DEPARTAMENTO VARCHAR(20) NOT NULL,
CONSTRAINT PK_DEPARTAMENTO PRIMARY KEY (ID_DEPARTAMENTO)
CONSTRAINT UK_DEPAR
);

--Creamos la tabla LOCALIDADES
CREATE TABLE LOCALIDADES(
ID_LOCALIDAD INTEGER NOT NULL,
NOMBRE_LOCALIDAD VARCHAR(20) NOT NULL,
ID_DEPARTAMENTO INTEGER    NOT NULL, --FK DEPARTAMENTO
    CONSTRAINT PK_LOCALIDAD PRIMARY KEY (ID_LOCALIDAD),
    CONSTRAINT FK_LOCALIDADES_IDDEPARTAMENTO FOREIGN KEY (ID_DEPARTAMENTO)
    REFERENCES DEPARTAMENTOS (ID_DEPARTAMENTO)  
);

--Agregamos ID_LOCALIDAD FK
ALTER TABLE DEPARTAMENTOS 
ADD(
ID_LOCALIDAD INTEGER NOT NULL,
CONSTRAINT FK_DEPARTAMENTOS_ID_LOCALIDAD FOREIGN KEY (ID_LOCALIDAD)
    REFERENCES LOCALIDADES (ID_LOCALIDAD)  
);

--creamos la tabla USUARIOS--
CREATE TABLE USUARIOS(
ID_USUARIO  INTEGER NOT NULL,                        --PRIMARY
NOMBRE_USUARIO  VARCHAR(30) NOT NULL,   --UNIQUE
CONTRASENA  VARCHAR(50) NOT NULL,
APELLIDOS    VARCHAR(30) NOT NULL,
NOMBRES    VARCHAR(30) NOT NULL,
FECHANACI   DATE    NOT NULL,
GENERO    VARCHAR(15) NOT NULL,
ID_DEPARTAMENTO INTEGER    NOT NULL,         --FK DEPARTAMENTOS
ID_LOCALIDAD   INTEGER NOT NULL,                   --FK LOCALIDADES
MAIL    VARCHAR(50) NOT NULL,                         --UNIQUE
TELEFONO    INTEGER NOT NULL, 
DOCUMENTO   VARCHAR(50) NOT NULL,           --UNIQUE
        CONSTRAINT PK_USUARIO  PRIMARY KEY (ID_USUARIO),
        CONSTRAINT UK_USUARIO_DOCUMENTO UNIQUE (DOCUMENTO),
        CONSTRAINT UK_USUARIO_MAIL UNIQUE (MAIL),
        CONSTRAINT UK_USUARIO_NOMBRE_USUARIO UNIQUE (NOMBRE_USUARIO),
        CONSTRAINT FK_USUARIO_IDDEPARTAMENTO FOREIGN KEY  (ID_DEPARTAMENTO)
        REFERENCES DEPARTAMENTOS (ID_DEPARTAMENTO),
        CONSTRAINT FK_USUARIO_ID_LOCALIDAD FOREIGN KEY  (ID_LOCALIDAD)
        REFERENCES LOCALIDADES (ID_LOCALIDAD)
);


--Creamos la tabla ITRS
CREATE TABLE ITR (
ID_ITR  INTEGER NOT NULL, 
NOMBRE_ITR VARCHAR(20) NOT NULL,
ID_DEPARTAMENTO INTEGER NOT NULL,
CONSTRAINT PK_ITR PRIMARY KEY (ID_ITR),
CONSTRAINT FK_ITR_IDDEPARTAMENTO FOREIGN KEY (ID_DEPARTAMENTO)
    REFERENCES DEPARTAMENTOS (ID_DEPARTAMENTO),
);

--Agregamos a USUARIOS el ID_ITR FK
ALTER TABLE USUARIOS 
ADD(
ID_ITR  INTEGER NOT NULL, 
CONSTRAINT FK_USUARIO_ID_ITR FOREIGN KEY  (ID_ITR)
        REFERENCES ITR (ID_ITR)  
);



-- Creamos la tabla ESTUDIANTES
CREATE TABLE ESTUDIANTES(
ID_ESTUDIANTE INTEGER NOT NULL,
ID_USUARIO  INTEGER NOT NULL,
GENERACION VARCHAR(20) NOT NULL,
CONSTRAINT FK_ESTUDIANTES_IDUSUARIO FOREIGN KEY (ID_USUARIO)
	REFERENCES USUARIOS (ID_USUARIO),
CONSTRAINT PK_ESTUDIANTE  PRIMARY KEY (ID_ESTUDIANTE)
);

-- Creamos la tabla TIPO_TUTOR
CREATE TABLE TIPO_TUTOR(
TIPO_TUTOR VARCHAR(10) NOT NULL,
ID_TIPOTUTOR INTEGER NOT NULL,
CONSTRAINT PK_TIPO_TUTOR PRIMARY (ID_TIPOTUTOR)
);

-- Creamos la tabla TUTORES
CREATE TABLE TUTORES (
ID_TUTOR INTEGER NOT NULL,
ID_USUARIO  INTEGER NOT NULL,
ID_TIPOTUTOR INTEGER NOT NULL,
AREA VARCHAR(20) NOT NULL,
CONSTRAINT PK_TUTOR  PRIMARY KEY (ID_TUTOR),
CONSTRAINT FK_TUTORES_IDUSUARIO FOREIGN KEY (ID_USUARIO)
	REFERENCES USUARIOS (ID_USUARIO),
	CONSTRAINT FK_TUTORES_TIPOTUTOR FOREIGN KEY(ID_TIPOTUTOR)
	REFERENCES TIPO_TUTOR (ID_TIPOTUTOR)
);

-- Creamos la tabla ANALISTA
CREATE TABLE ANALISTAS (
ID_ANALISTA INTEGER NOT NULL,
ID_USUARIO  INTEGER NOT NULL,
CONSTRAINT FK_ANALISTA_IDUSUARIO FOREIGN KEY (ID_USUARIO)
	REFERENCES USUARIOS (ID_USUARIO),
CONSTRAINT PK_ANALISTA  PRIMARY KEY (ID_ANALISTA)
);

-- Creamos la tabla EVENTOS
CREATE TABLE EVENTOS(
ID_EVENTO INTEGER NOT NULL,
TITULO VARCHAR(30) NOT NULL,	
INICIO TIMESTAMP NOT NULL,
FIN TIMESTAMP NOT NULL,
ID_USUARIO  INTEGER NOT NULL,
CONSTRAINT PK_EVENTO PRIMARY KEY (ID_EVENTO),
CONSTRAINT FK_EVENTO_IDUSUARIO FOREIGN KEY (ID_USUARIO)
	REFERENCES USUARIOS (ID_USUARIO)
);

-- Creamos la tabla RESPONSABILIDADES
CREATE TABLE RESPONSABILIDADES(
ID_EVENTO INTEGER NOT NULL,--FK
ID_RESPONSABILIDAD INTEGER NOT NULL,--PK
ID_TUTOR INTEGER NOT NULL,--FK
CONSTRAINT FK_RESPONSABILIDADES_ID_EVENTO FOREIGN KEY (ID_EVENTO)
	REFERENCES EVENTOS (ID_EVENTO),
CONSTRAINT PK_RESPONSABILIDADES PRIMARY KEY (ID_RESPONSABILIDAD),
CONSTRAINT FK_RESPONSABILIDADES_ID_TUTOR FOREIGN KEY (ID_TUTOR)
	REFERENCES TUTORES (ID_TUTOR)
);


-- Agregamos a la tabla EVENTOS el ID_RESPONSABILIDAD FK
ALTER TABLE EVENTOS
ADD(
ID_RESPONSABILIDAD INTEGER NOT NULL,
CONSTRAINT FK_EVENTOS_ID_RESPONSABILIDAD FOREIGN KEY (ID_RESPONSABILIDAD)
	REFERENCES RESPONSABILIDADES (ID_RESPONSABILIDAD)
);

-- Agregamos a la tabla TUTORES el ID_RESPONSABILIDAD FK
ALTER TABLE TUTORES
ADD(
ID_RESPONSABILIDAD INTEGER NOT NULL,
CONSTRAINT FK_TUTORES_ID_RESPONSABILIDAD FOREIGN KEY (ID_RESPONSABILIDAD)
	REFERENCES RESPONSABILIDADES (ID_RESPONSABILIDAD)
);

-- Creamos la tabla GESTIONES
CREATE TABLE GESTIONES (
ID_ANALISTA INTEGER NOT NULL,
ID_GESTION INTEGER NOT NULL,
ID_EVENTO INTEGER NOT NULL,
CONSTRAINT GESTION_ID_GESTION PRIMARY KEY (ID_GESTION),
CONSTRAINT FK_GESTION_ID_ANALISTA FOREIGN KEY (ID_ANALISTA)
	REFERENCES ANALISTAS (ID_ANALISTA),
CONSTRAINT FK_GESTION_ID_EVENTO FOREIGN KEY (ID_EVENTO)
	REFERENCES EVENTOS (ID_EVENTO)
);

--Agregamos a la tabla ANALISTAS el ID_GESTION FK
ALTER TABLE ANALISTAS
ADD(
ID_GESTION INTEGER NOT NULL,
CONSTRAINT FK_ANALISTAS_ID_GESTION FOREIGN KEY (ID_GESTION)
	REFERENCES GESTIONES (ID_GESTION)
);

--Agregamos a la tabla EVENTOS el ID_GESTION FK
ALTER TABLE EVENTOS
ADD(
ID_GESTION INTEGER NOT NULL,
CONSTRAINT FK_EVENTOS_ID_GESTION FOREIGN KEY (ID_GESTION)
	REFERENCES GESTIONES (ID_GESTION)
);


-- Creamos la tabla TIPO_ASISTENCIA
CREATE TABLE TIPO_ASISTENCIA(
ID_TIPO_ASISTENCIA INTEGER NOT NULL,
TIPO_ASISTENCIA VARCHAR(20) NOT NULL,
CONSTRAINT PK_ID_TIPO_ASISTENCIA PRIMARY KEY (ID_TIPO_ASISTENCIA)
);



-- Creamos la tabla CONVOCATORIA_ASISTENCIA
CREATE TABLE CONVOCATORIA_ASISTENCIA(
ID_EVENTO INTEGER NOT NULL,
ID_CONVOCATORIA_ASISTENCIA INTEGER NOT NULL,
ID_ESTUDIANTE INTEGER NOT NULL,
ID_TIPO_ASISTENCIA INTEGER NOT NULL,
CALIFICACION VARCHAR(30) NOT NULL,
CONSTRAINT FK_CONVOCATORIA_ASISTENCIA FOREIGN KEY (ID_ESTUDIANTE)
	REFERENCES ESTUDIANTES (ID_ESTUDIANTE),
CONSTRAINT PK_CONVOCATORIA_ASISTENCIA PRIMARY KEY (ID_CONVOCATORIA_ASISTENCIA),
CONSTRAINT FK_ID_TIPO_ASISTENCIA FOREIGN KEY (ID_TIPO_ASISTENCIA)
	REFERENCES TIPO_ASISTENCIA (ID_TIPO_ASISTENCIA),
CONSTRAINT FK_ID_EVENTO FOREIGN KEY (ID_EVENTO)
	REFERENCES EVENTOS (ID_EVENTO)
);

-- Agregamos a la tabla EVENTOS ID_CONVOCATORIA y esta es FK de CONVOCATORIA_ASISTENCIA
ALTER TABLE EVENTOS
ADD(
ID_CONVOCATORIA_ASISTENCIA INTEGER NOT NULL,
CONSTRAINT FK_ID_CONVOCATORIA_ASISTENCIA FOREIGN KEY (ID_CONVOCATORIA_ASISTENCIA)
	REFERENCES CONVOCATORIA_ASISTENCIA (ID_CONVOCATORIA_ASISTENCIA)
);

-- Creamos la tabla JUSTIFICACIONES
CREATE TABLE JUSTIFICACIONES (
ID_JUSTIFICACION INTEGER NOT NULL,
DETALLE VARCHAR(50) NOT NULL,
FECHA_HORA TIMESTAMP NOT NULL,
ID_ESTUDIANTE INTEGER NOT NULL,
ID_EVENTO INTEGER NOT NULL,
CONSTRAINT PK_JUSTIFICACIONES PRIMARY KEY (ID_JUSTIFICACION),
CONSTRAINT FK_JUSTIFICACIONES_ID_ESTUDIANTE FOREIGN KEY (ID_ESTUDIANTE)
	REFERENCES ESTUDIANTES (ID_ESTUDIANTE),
CONSTRAINT FK_JUSTIFICACIONES_ID_EVENTO FOREIGN KEY (ID_EVENTO)
	REFERENCES EVENTOS (ID_EVENTO)
);

ALTER TABLE EVENTOS
ADD(
ID_JUSTIFICACION INTEGER NOT NULL,
CONSTRAINT FK_EVENTOS_ID_JUSTIFICACION  FOREIGN KEY (ID_JUSTIFICACION)
	REFERENCES JUSTIFICACIONES (ID_JUSTIFICACION)
);



-- Agregamos a la tabla ESTUDIANTES el ID_JUSTIFICACION siendo FK de la tabla JUSTIFICACIONES
ALTER TABLE ESTUDIANTES
ADD(
ID_JUSTIFICACION INTEGER NOT NULL,
CONSTRAINT FK_ID_JUSTIFICACION FOREIGN KEY (ID_JUSTIFICACION)
	REFERENCES JUSTIFICACIONES (ID_JUSTIFICACION)
);

-- Creamos la tabla ACCION_JUSTIFICACIONES
CREATE TABLE ACCION_JUSTIFICACION(
ID_ACCION_JUSTIFICACION INTEGER NOT NULL,
ID_ANALISTA INTEGER NOT NULL,
ID_JUSTIFICACION INTEGER NOT NULL,
CONSTRAINT FK_ACCION_JUSTIFICACION_ID_JUSTIFICACION FOREIGN KEY (ID_JUSTIFICACION)
	REFERENCES JUSTIFICACIONES (ID_JUSTIFICACION),
CONSTRAINT PK_ACCION_JUSTIFICACION PRIMARY KEY (ID_ACCION_JUSTIFICACION),
CONSTRAINT FK_ACCION_JUSTIFICACION_ID_ANALISTA FOREIGN KEY (ID_ANALISTA)
	REFERENCES ANALISTAS (ID_ANALISTA)
);

--Agregamos a la tabla JUSTIFICACIONES el ID_ACCION_JUSTIFICACION
ALTER TABLE JUSTIFICACIONES
ADD(
ID_ACCION_JUSTIFICACION INTEGER NOT NULL,
CONSTRAINT FK_JUSTIFICACION_ID_ACCION_JUSTIFICACION FOREIGN KEY (ID_ACCION_JUSTIFICACION)
	REFERENCES ACCION_JUSTIFICACION (ID_ACCION_JUSTIFICACION)
);

-- Creamos la tabla CONSTANCIAS
CREATE TABLE CONSTANCIAS(
ID_CONSTANCIA INTEGER NOT NULL,
DETALLE VARCHAR(50) NOT NULL,
FECHA_HORA TIMESTAMP NOT NULL,
ID_ESTUDIANTE INTEGER NOT NULL,
ID_EVENTO INTEGER NOT NULL,
CONSTRAINT PK_CONSTANCIAS PRIMARY KEY (ID_CONSTANCIA),
CONSTRAINT FK_CONSTANCIAS_ID_ESTUDIANTE FOREIGN KEY (ID_ESTUDIANTE)
	REFERENCES ESTUDIANTES (ID_ESTUDIANTE),
CONSTRAINT FK_CONSTANCIAS_ID_EVENTO FOREIGN KEY (ID_EVENTO)
	REFERENCES EVENTOS (ID_EVENTO)
);

-- Agregamos a la tabla ESTUDIANTES el ID_CONSTANCIA siendo esta FK de la tabla CONSTANCIAS
ALTER TABLE ESTUDIANTES
ADD(
ID_CONSTANCIA INTEGER NOT NULL,
CONSTRAINT FK_ID_CONSTANCIA FOREIGN KEY (ID_CONSTANCIA)
	REFERENCES CONSTANCIAS (ID_CONSTANCIA)
);

ALTER TABLE EVENTOS
ADD(
ID_CONSTANCIA INTEGER NOT NULL,
CONSTRAINT FK_EVENTOS_ID_CONSTANCIA  FOREIGN KEY (ID_CONSTANCIA)
	REFERENCES CONSTANCIAS (ID_CONSTANCIA)
);


-- Creamos la tabla ACCION_CONSTANCIA
CREATE TABLE ACCION_CONSTANCIA(
ID_ACCION_CONSTANCIA INTEGER NOT NULL,
ID_ANALISTA INTEGER NOT NULL,
ID_CONSTANCIA INTEGER NOT NULL,
CONSTRAINT FK_ACCION_CONSTANCIA_ID_CONSTANCIA FOREIGN KEY (ID_CONSTANCIA)
	REFERENCES CONSTANCIAS (ID_CONSTANCIA),
CONSTRAINT PK_ACCION_CONSTANCIA PRIMARY KEY (ID_ACCION_CONSTANCIA),
CONSTRAINT FK_ACCION_CONSTANCIA_ID_ANALISTA FOREIGN KEY (ID_ANALISTA)
	REFERENCES ANALISTAS (ID_ANALISTA)
);

--Argegamos a la tabla CONSTANCIAS el ID_ACCION_CONSTANCIA FK de la tabla ACCION_CONSTANCIA
ALTER TABLE CONSTANCIAS
ADD(
ID_ACCION_CONSTANCIA INTEGER NOT NULL,
CONSTRAINT FK_CONSTANCIA_ID_ACCION_CONSTANCIA FOREIGN KEY (ID_ACCION_CONSTANCIA)
	REFERENCES ACCION_CONSTANCIA (ID_ACCION_CONSTANCIA)
);








-- Creamos la tabla RECLAMOS
CREATE TABLE RECLAMOS (
ID_RECLAMO INTEGER NOT NULL,
DETALLE VARCHAR(50) NOT NULL,
FECHA_HORA TIMESTAMP NOT NULL,
ID_ESTUDIANTE INTEGER NOT NULL,
ID_EVENTO INTEGER NOT NULL,
CONSTRAINT PK_RECLAMOS PRIMARY KEY (ID_RECLAMO),
CONSTRAINT FK_RECLAMOS_ID_ESTUDIANTE FOREIGN KEY (ID_ESTUDIANTE)
	REFERENCES ESTUDIANTES (ID_ESTUDIANTE),
CONSTRAINT FK_RECLAMOS_JUSTIFICACIONES_ID_EVENTO FOREIGN KEY (ID_EVENTO)
	REFERENCES EVENTOS (ID_EVENTO)
);

--Agregamos a la tabla ESTUDIANTES el ID_RECLAMO FK de la tabla RECLAMOS
ALTER TABLE ESTUDIANTES
ADD(
ID_RECLAMO INTEGER NOT NULL,
CONSTRAINT FK_ESTUDIANTES_ID_RECLAMO FOREIGN KEY (ID_RECLAMO)
	REFERENCES RECLAMOS (ID_RECLAMO)
);

ALTER TABLE EVENTOS
ADD(
ID_RECLAMO INTEGER NOT NULL,
CONSTRAINT FK_EVENTOS_ID_RECLAMO  FOREIGN KEY (ID_RECLAMO)
	REFERENCES RECLAMOS (ID_RECLAMO)
);


-- Creamos la tabla ACCION_RECLAMO
CREATE TABLE ACCION_RECLAMO(
ID_ACCION_RECLAMO INTEGER NOT NULL,
ID_ANALISTA INTEGER NOT NULL,
ID_RECLAMO INTEGER NOT NULL,
CONSTRAINT FK_ACCION_RECLAMO_ID_RECLAMO FOREIGN KEY (ID_RECLAMO)
	REFERENCES RECLAMOS (ID_RECLAMO),
CONSTRAINT PK_ACCION_RECLAMO PRIMARY KEY (ID_ACCION_RECLAMO),
CONSTRAINT FK_ACCION_RECLAMO_ID_ANALISTA FOREIGN KEY (ID_ANALISTA)
	REFERENCES ANALISTAS (ID_ANALISTA)
);

-- Agregamos a la tabla RECLAMOS el ID_ACCION_RECLAMO
ALTER TABLE RECLAMOS
ADD(
ID_ACCION_RECLAMO INTEGER NOT NULL,
CONSTRAINT FK_RECLAMO_ID_ACCION_RECLAMO FOREIGN KEY (ID_ACCION_RECLAMO)
	REFERENCES ACCION_RECLAMO (ID_ACCION_RECLAMO)
);




-- Hacemos commit para que quede guardado en la base de datos.
commit;


/*====================================*/
--  	Creacion de secuencias
/*====================================*/
--Borrado de secuencias

DROP SEQUENCE SEQ_ID_USUARIO;
DROP SEQUENCE SEQ_ID_TUTOR;
DROP SEQUENCE SEQ_ID_TIPOTUTOR;
DROP SEQUENCE SEQ_ID_TIPO_ASISTENCIA;
DROP SEQUENCE SEQ_ID_RECLAMO;
DROP SEQUENCE SEQ_ID_LOCALIDAD;
DROP SEQUENCE SEQ_ID_JUSTIFICACION;
DROP SEQUENCE SEQ_ID_ITR;
DROP SEQUENCE SEQ_ID_EVENTO;
DROP SEQUENCE SEQ_ID_ESTUDIANTE;
DROP SEQUENCE SEQ_ID_DEPARTAMENTO;
DROP SEQUENCE SEQ_ID_CONVOCATORIA_ASISTENCIA;
DROP SEQUENCE SEQ_ID_CONSTANCIA;
DROP SEQUENCE SEQ_ID_ANALISTA;
DROP SEQUENCE SEQ_ID_ACCION_RECLAMO;
DROP SEQUENCE SEQ_ID_ACCION_JUSTIFICACION;
DROP SEQUENCE SEQ_ID_ACCION_CONSTANCIA;



--Creamos las secuencias para las id de cada tabla

CREATE SEQUENCE SEQ_ID_USUARIO START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_TUTOR START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_TIPOTUTOR START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_TIPO_ASISTENCIA START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_RECLAMO START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_LOCALIDAD START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_JUSTIFICACION START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_ITR START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_EVENTO START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_ESTUDIANTE START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_DEPARTAMENTO START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_CONVOCATORIA_ASISTENCIA START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_CONSTANCIA START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_ANALISTA START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_ACCION_RECLAMO START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_ACCION_JUSTIFICACION START WITH 1 INCREMENT BY 1 NOCACHE;
CREATE SEQUENCE SEQ_ID_ACCION_CONSTANCIA START WITH 1 INCREMENT BY 1 NOCACHE;

